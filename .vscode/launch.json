{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/FhirLoader.Tool/bin/Debug/net6.0/FhirLoader.Tool",
            "args": [
                "--blob",
                "https://ahdssampledata.blob.core.windows.net/fhir/uscore-testing-data-09-17-2022/",
                "--fhir",
                "https://workspace-fhirservice.fhir.azurehealthcareapis.com"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}